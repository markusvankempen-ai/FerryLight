# FerryLight App Environment Configuration
# Copy this file to .env.local and update with your actual values

# API Configuration
REACT_APP_API_USERNAME=your_api_username
REACT_APP_API_PASSWORD=your_api_password

# API Endpoints
REACT_APP_FERRY_API_URL=https://your-api-domain.com/ferry-endpoint
REACT_APP_WEATHER_API_URL=https://your-api-domain.com/weather-endpoint

# Application Settings
REACT_APP_APP_NAME=FerryLight
REACT_APP_VERSION=1.0.0

# Optional: Enable debug mode
REACT_APP_DEBUG=false

# Admin Panel Configuration
REACT_APP_ADMIN_USERNAME=admin
REACT_APP_ADMIN_PASSWORD=your_admin_password
